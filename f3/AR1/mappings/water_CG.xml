<cg_molecule>
  <name>SOL</name> <!-- the name of the molecule after mapping -->
  <ident>SOL</ident> <!-- the name of the molecule in atomistic topology -->
  <topology>
    <cg_beads>
      <cg_bead>
        <name>SOL</name>
        <type>SOL</type>
        <mapping>UNITY</mapping>
        <beads>
          1:SOL:SOL
        </beads>
      </cg_bead>
    </cg_beads>
  </topology>
  <maps>
    <map>
      <name>UNITY</name>
      <weights>1</weights>
    </map>
  </maps>
</cg_molecule>
