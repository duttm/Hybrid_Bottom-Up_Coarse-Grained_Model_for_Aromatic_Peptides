<cg>
  <!-- force matching parameters -->
  <fmatch>
    <!-- use constraint LS solver -->
    <constrainedLS>true</constrainedLS>
    <!-- Number of frames for block averaging -->
    <frames_per_block>1000</frames_per_block>
  </fmatch>
    <!-- settings for gromacs .xvg table -->
  <inverse>
    <gromacs>
      <table_bins>0.001</table_bins>
      <pot_max>1000000</pot_max>
      <table_end>2.3</table_end>
    </gromacs>
  </inverse>

  <!-- non-bonded interactions -->
  <!-- fmatch.min values are 1) first nonzero (trace) 2) first .001 3) .1 4) .2-->
  <!-- 5) halfmax for first local max -->
  <non-bonded>
    <name>COO-COO</name>
    <type1>COO</type1>
    <type2>COO</type2>
    <min>0</min>
    <max>1.9</max>
    <step>0.010</step>
    <fmatch>
      <!-- <min>0.360</min> -->
      <!-- <min>0.400</min> -->
      <min>0.440</min>
      <!-- <min>0.460</min> -->
      <!-- <min>0.520</min> -->
      <max>1.200</max>
      <step>0.03</step>
      <out_step>0.01</out_step>
    </fmatch>
  </non-bonded>

</cg>
